   "source": [
    "# Train the model with Cross Validation\n",
    "for train, validate in kf.split(X_train):\n",
    "    net.fit(\n",
    "        X_train[train],\n",
    "        y_train_encoded[train],\n",
    "        epochs=50,\n",
    "        batch_size=128,\n",
    "        validation_data=(X_train[validate], y_train_encoded[validate]),\n",
    "    )"
   ]
  },
